{
	"name": "orisai/nette-mail",
	"description": "Extras for nette/mail",
	"keywords": [
		"debug",
		"mail",
		"mailer",
		"nette",
		"orisai",
		"tracy"
	],
	"type": "library",
	"license": "MPL-2.0",
	"homepage": "https://github.com/orisai/nette-mail",
	"authors": [
		{
			"name": "Marek BartoÅ¡",
			"homepage": "https://github.com/mabar"
		}
	],
	"require": {
		"php": ">=7.4.0 <8.3.0",
		"latte/latte": "^2.9.6|^3.0.0",
		"nette/finder": "^2.6.0|^3.0.0",
		"nette/http": "^3.1.0",
		"nette/mail": "^3.1.10|^4.0.0",
		"nette/utils": "^3.1.6|^4.0.0",
		"orisai/clock": "^1.2.0",
		"orisai/exceptions": "^1.1.0",
		"orisai/nette-di": "^1.3.0",
		"symfony/polyfill-php80": "^1.28"
	},
	"require-dev": {
		"brianium/paratest": "^6.3.0",
		"infection/infection": "^0.26.0",
		"orisai/coding-standard": "^3.0.0",
		"orisai/nette-http": "^1.1.0",
		"orisai/vfs": "^1.0.0",
		"phpstan/extension-installer": "^1.0.0",
		"phpstan/phpstan": "^1.0.0",
		"phpstan/phpstan-deprecation-rules": "^1.0.0",
		"phpstan/phpstan-nette": "^1.2.0",
		"phpstan/phpstan-phpunit": "^1.0.0",
		"phpstan/phpstan-strict-rules": "^1.0.0",
		"phpunit/phpunit": "^9.5.0",
		"staabm/annotate-pull-request-from-checkstyle": "^1.7.0",
		"symfony/console": "^5.2.6|^6.3.0",
		"tracy/tracy": "^2.9.8|^2.10.0"
	},
	"autoload": {
		"psr-4": {
			"OriNette\\Mail\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\OriNette\\Mail\\": "tests"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"sort-packages": true,
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true,
			"infection/extension-installer": true,
			"phpstan/extension-installer": true
		}
	}
}
